<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

  <rdfs:Class rdf:about="http://ryac.ca/ns/adb#Population">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Population</rdfs:label>
    <rdfs:comment>Population details of a country</rdfs:comment>
  </rdfs:Class>

  <rdf:Property rdf:about="http://ryac.ca/ns/adb#population_density">
    <rdfs:label>Population Density</rdfs:label>
    <rdfs:comment>Persons per square kilometer.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://ryac.ca/ns/adb#population_urban">
    <rdfs:label>Urban Population</rdfs:label>
    <rdfs:comment>Percent of total population.</rdfs:comment>
  </rdf:Property>

  <rdf:Property rdf:about="http://ryac.ca/ns/adb#population_annual_change">
    <rdfs:label>Annual change in population</rdfs:label>
    <rdfs:comment>Annual change is in percent.</rdfs:comment>
  </rdf:Property>

