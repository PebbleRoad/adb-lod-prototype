@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix adb: <http://ryac.ca/ns/adb#> .

adb:Population a rdfs:Class, owl:Class ;
  rdfs:label "Population" ;
  rdfs:comment "Population details of a country" .

adb:population_density
  rdf:type rdf:Property ;
  rdfs:label "Population Density" ;
  rdfs:comment "Persons per square kilometer." .

adb:population_urban
  rdf:type rdf:Property ;
  rdfs:label "Urban Population" ;
  rdfs:comment "Percent of total population." .

adb:population_annual_change
  rdf:type rdf:Property ;
  rdfs:label "Annual change in population" ;
  rdfs:comment "Annual change is in percent." .